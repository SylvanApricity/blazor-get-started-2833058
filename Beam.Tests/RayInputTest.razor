@inherits TestComponentBase

<SnapshotTest Description="Ray input with parameter" Setup="@Setup">
    <TestInput>
        <Beam.Client.Pages.RayInput newRayText="test text"/>
    </TestInput>
    <ExpectedOutput>
        <p>
        </p>
        <div class="card">
            <div class="card-header">Cast a new ray</div>
            <div class="card-body">
                <div class="card">
                    <div class="card-body">
                        <span class="card-title">Cast a New Ray:</span>
                        <div class="input-group">
                            <input class="form-control" value="test text">
                            <div diff:ignore></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <p></p>
    </ExpectedOutput>
    @code{
        void Setup(SnapshotTest test)
        {
            test.Services.AddSingleton<IDataService, TestDataService>();
        }
    }
</SnapshotTest>